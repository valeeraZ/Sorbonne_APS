[
  CONST xss (vec (vec bool)) (alloc 3);
  VAR i int;
  SET i 0;
  WHILE (lt i (len xss))
    [
      SET (nth xss i) (alloc (add i 1));
      SET i (add i 1)
    ];
  SET (nth (nth xss 1) 1) true;
  SET (nth (nth xss 2) 2) true;
  IF (and (nth (nth xss 1) 1) (nth (nth xss 2) 2)) [
    ECHO 42
  ]
  [
    ECHO 41
  ] 
]